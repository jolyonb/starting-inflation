Cell["\<\
Sort all data into the grid, then flatten into a list for processing.\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"grid", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"i", ",", "j", ",", 
       RowBox[{"{", "}"}]}], "}"}], " ", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "\[Phi]0vals"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "\[Phi]0dotvals"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"AppendTo", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"grid", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"FindIndex", "[", 
          RowBox[{
           RowBox[{"i", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", "\[Phi]0vals"}], "]"}], ",", 
         
         RowBox[{"FindIndex", "[", 
          RowBox[{
           RowBox[{"i", "[", 
            RowBox[{"[", "2", "]"}], "]"}], ",", "\[Phi]0dotvals"}], "]"}], 
         ",", "3"}], "]"}], "]"}], ",", "\[IndentingNewLine]", "i"}], "]"}], 
    "\[IndentingNewLine]", ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "working"}], "}"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"flattenedgrid", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{"grid", ",", "1"}], "]"}]}], ";"}]}], "Input"],

Cell["\<\
Function to extract statistics from an element of flattenedgrid.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"extractstats", "[", "data_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"efolds", ",", "mean", ",", "sd"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"efolds", "=", 
      RowBox[{"data", "[", 
       RowBox[{"[", 
        RowBox[{"3", ",", ";;", ",", "7"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"mean", "=", 
      RowBox[{"Mean", "[", "efolds", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"sd", "=", 
      RowBox[{"StandardDeviation", "[", "efolds", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"mean", ",", 
       RowBox[{"mean", "+", "sd"}], ",", 
       RowBox[{"mean", "-", "sd"}], ",", "sd", ",", 
       RowBox[{"mean", "+", 
        RowBox[{"2", " ", "sd"}]}], ",", " ", 
       RowBox[{"mean", "-", 
        RowBox[{"2", " ", "sd"}]}]}], "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input"],

Cell["Apply statistics to everything!", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"stats", "=", "flattenedgrid"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "\[LessEqual]", 
    RowBox[{"Length", "[", "stats", "]"}]}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"stats", "[", 
     RowBox[{"[", "i", "]"}], "]"}], "=", 
    RowBox[{
     RowBox[{"stats", "[", 
      RowBox[{"[", 
       RowBox[{"i", ",", 
        RowBox[{"1", ";;", "2"}]}], "]"}], "]"}], "~", "Join", "~", 
     RowBox[{"extractstats", "[", 
      RowBox[{"stats", "[", 
       RowBox[{"[", "i", "]"}], "]"}], "]"}]}]}]}], "\[IndentingNewLine]", 
  "]"}]}], "Input"],

